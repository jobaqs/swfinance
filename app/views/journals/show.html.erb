<div class="page-header">
  <%= link_to journals_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_journal_path(@journal), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show journal</h1>
</div>

<dl class="dl-horizontal">
  <dt>Journal date:</dt>
  <dd><%= @journal.journal_date %></dd>

  <dt>Payee:</dt>
  <dd><%= @journal.payee %></dd>

  <dt>Description:</dt>
  <dd><%= @journal.description %></dd>

  <dt>Account:</dt>
  <dd><%= @journal.account_id %></dd>

  <dt>Amount:</dt>
  <dd><%= @journal.amount %></dd>

  <dt>User:</dt>
  <dd><%= @journal.user_id %></dd>

  <dt>Is posted:</dt>
  <dd><%= @journal.is_posted %></dd>

</dl>
